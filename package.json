{
  "name": "yumeshop-frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:eslint & yarn lint:prettier",
    "lint:eslint": "eslint --ext .ts,.tsx,.js .",
    "lint:prettier": "prettier --ignore-path .prettierignore  --check '**/*.{js,ts,tsx,css}'",
    "format": "yarn format:eslint & yarn format:prettier",
    "format:eslint": "eslint --fix --ext .ts,.tsx,.js .",
    "format:prettier": "prettier --ignore-path .prettierignore  --write '**/*.{js,ts,tsx,css}'",
    "fix": "yarn format && yarn lint:fix",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@next/eslint-plugin-next": "^12.1.0",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.3.0",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ress": "^4.0.0",
    "typescript": "4.5.5"
  }
